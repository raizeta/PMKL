{% extends "LayoutBundle:FrontEnd:index.html.twig" %}
{% block slider %}
{% endblock slider %}
{% block content %}
<section id="services" class="services service-section">
  <div class="container">
    <div class="row">
      {% for entity in entities %}
      <div class="col-md-6 col-sm-6 services text-center"> 
        <span class="{{entity.iconImage}}"></span>
        <div class="services-content">
          <h5>{{entity.judulContent|upper}}</h5>
          <p align="justify">
            {{entity.isiContent}}...<a href="">Read More</a>
          </p>

        </div>
      </div>
      {% endfor %}
    </div>
  </div>
</section>
{% endblock content %}