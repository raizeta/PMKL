{{form_start(edit_form, {'attr': {'id': 'editTblpengeluaran'}})}}
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header bg-light-blue">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Edit Tblpengeluaran</h4>
      </div>
      <div class="modal-body">
        <div class="form-group">
          <label>Yang Menerima</label>
            {{form_widget(edit_form.namaPemakai)}}
        </div>
        <div class="form-group">
          <label>Yang Menyerahkan</label>
            {{form_widget(edit_form.namaPenyerah)}}
        </div>
        <div class="form-group">
          <label>Tanggal Penyerahan</label>
            {{form_widget(edit_form.tanggalpenyerahan)}}
        </div>
        <div class="form-group">
          <label>Catatan</label>
            {{form_widget(edit_form.catatan)}}
        </div>
        <div class="form-group">
          <label>Type Pengeluaran</label>
            {{form_widget(edit_form.typePengeluaran)}}
        </div>
        <div class="form-group">
          <label>Jumlah Pengeluaran</label>
            {{form_widget(edit_form.jumlah)}}
        </div>
      </div>
      <div class="modal-footer bg-light-blue">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
        <button type="submit" class="btn btn-success">Save</button>
      </div>
    </div>
  </div>
{{form_end(edit_form)}}