{% extends 'base.html.twig' %}

{% block body %}
    <h1>Forumstatus</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ forumStatus.id }}</td>
            </tr>
            <tr>
                <th>Namastatus</th>
                <td>{{ forumStatus.namaStatus }}</td>
            </tr>
            <tr>
                <th>Deskripsistatus</th>
                <td>{{ forumStatus.deskripsiStatus }}</td>
            </tr>
            <tr>
                <th>Statusdisplay</th>
                <td>{% if forumStatus.statusDisplay %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Createat</th>
                <td>{% if forumStatus.createAt %}{{ forumStatus.createAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updateat</th>
                <td>{% if forumStatus.updateAt %}{{ forumStatus.updateAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('forumstatus_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('forumstatus_edit', { 'id': forumStatus.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
