{% extends 'base.html.twig' %}

{% block body %}
    <h1>Fostypepemasukan</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fosTypepemasukan.id }}</td>
            </tr>
            <tr>
                <th>Namatype</th>
                <td>{{ fosTypepemasukan.namaType }}</td>
            </tr>
            <tr>
                <th>Deskripsitype</th>
                <td>{{ fosTypepemasukan.deskripsiType }}</td>
            </tr>
            <tr>
                <th>Statusdisplay</th>
                <td>{% if fosTypepemasukan.statusDisplay %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Createat</th>
                <td>{% if fosTypepemasukan.createAt %}{{ fosTypepemasukan.createAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updateat</th>
                <td>{% if fosTypepemasukan.updateAt %}{{ fosTypepemasukan.updateAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('fostypepemasukan_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('fostypepemasukan_edit', { 'id': fosTypepemasukan.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
