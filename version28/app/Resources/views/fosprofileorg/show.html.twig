{% extends 'base.html.twig' %}

{% block body %}
    <h1>Fosprofileorg</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fosProfileorg.id }}</td>
            </tr>
            <tr>
                <th>Judulcontent</th>
                <td>{{ fosProfileorg.judulContent }}</td>
            </tr>
            <tr>
                <th>Slugcontent</th>
                <td>{{ fosProfileorg.slugContent }}</td>
            </tr>
            <tr>
                <th>Summarycontent</th>
                <td>{{ fosProfileorg.summaryContent }}</td>
            </tr>
            <tr>
                <th>Isicontent</th>
                <td>{{ fosProfileorg.isiContent }}</td>
            </tr>
            <tr>
                <th>Statusdisplay</th>
                <td>{% if fosProfileorg.statusDisplay %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Createat</th>
                <td>{% if fosProfileorg.createAt %}{{ fosProfileorg.createAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updateat</th>
                <td>{% if fosProfileorg.updateAt %}{{ fosProfileorg.updateAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Imagename</th>
                <td>{{ fosProfileorg.imageName }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('fosprofileorg_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('fosprofileorg_edit', { 'id': fosProfileorg.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
