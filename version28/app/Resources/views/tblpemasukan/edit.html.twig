{{form_start(edit_form, {'attr': {'id': 'editTblpemasukan'}})}}
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header bg-light-blue">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Edit Tblpemasukan</h4>
      </div>
      <div class="modal-body">
        <div class="form-group">
          <label>Nama Penyetor</label>
            {{form_widget(edit_form.catatan)}}
        </div>
        <div class="form-group">
          <label>Nama Penerima</label>
            {{form_widget(edit_form.diterimaOleh)}}
        </div>
        <div class="form-group">
          <label>Nama Donatur</label>
            {{form_widget(edit_form.donatur)}}
        </div>
        <div class="form-group">
          <label>Type Pemasukan</label>
            {{form_widget(edit_form.typePemasukan)}}
        </div>
        <div class="form-group">
          <label>Tanggal Diterima</label>
            {{form_widget(edit_form.diterimaTanggal)}}
        </div>
        <div class="form-group">
          <label>Jumlah Pemasukan</label>
            {{form_widget(edit_form.jumlahPemasukan)}}
        </div>
      </div>
      <div class="modal-footer bg-light-blue">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
        <button type="submit" class="btn btn-success">Save</button>
      </div>
    </div>
  </div>
{{form_end(edit_form)}}